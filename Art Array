#include <iostream>
#include <string>
using namespace std;
int main()
{
	string art[5][5] = 
	{
	{"_","_","_","_","_"},
	{"_", "O", "_", "O", "_"},
	{"_", "@", "@", "@", "_"},
	{"_", "^", "^", "^", "_"},
	{"_", "V", "V", "V","_" }
	};

	for (int x = 0; x < 5; x++)
	{
		cout << endl;
		for (int y = 0; y < 5; y++)
			cout << art[x][y];
	}
	cout << endl;
}
