#include <iostream>

using namespace std;
int main() 
{
	int x[5];
	cout << "Enter 5 numbers: \n";
	for (int i = 0; i < 5; i++) 
		{
			cin >> x[i];
		}
	cout << "First number: " << x[0] << endl;
	cout << "Second number: " << x[1] << endl;
	cout << "Third number: " << x[2] << endl;
	cout << "Forth number: " << x[3] << endl;
	cout << "Last number: " << x[4];
	return 0;
}
