#include <iostream>
#include <array>
#include <algorithm>
using namespace std;
int main()
{
	int x[10];
	cout << "Enter 10 numbers: \n";
	for (int i = 0; i < 10; i++)
	{
		cin >> x[i];
	}
	for (int i = 1;i < 10; i++)
	{
		if (x[0] < x[i])
			x[0] = x[i];
	}
	cout << "Largest Number = " << x[0];

	return 0;
}
